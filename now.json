{
	"version": 2,
	"github": {
		"enabled": true
	},
	"builds": [
		{ "src": "deploy.sh", "use": "@now/static-build" },
		{ "src": ".well-known/keybase.txt", "use": "@now/static" },
		{ "src": "./**", "use": "@now/static" }
	],
	"routes": [
		{ "src": "(.*[^//])",
		  "status": 301,
		  "headers": { "Location": "$1/" }
		}
	]
}
